    <function name="Main" type="None" variable="">
        <parameters/>
        <body>
            <declare name="EscolhaLanche" type="Integer" array="False" size=""/>
            <output expression="&quot;Bem-vindo ao MacDonalds! Nossas op&#231;&#245;es no card&#225;pio s&#227;o: &quot;" newline="True"/>
            <output expression="&quot;1 = BigMac&quot;" newline="True"/>
            <output expression="&quot;2 = Quarteir&#227;o&quot;" newline="True"/>
            <output expression="&quot;3 = MacChicken&quot;" newline="True"/>
            <output expression="&quot;4 = Cheddar MacMelt&quot;" newline="True"/>
            <output expression="&quot;5 = MacMax&quot;" newline="True"/>
            <output expression="&quot;Por favor, informe o n&#250;mero do seu pedido:&quot;" newline="True"/>
            <input variable="EscolhaLanche"/>
            <if expression="EscolhaLanche=1">
                <then>
                    <output expression="&quot;Escolha confirmada: BigMac.&quot;" newline="True"/>
                </then>
                <else>
                    <if expression="EscolhaLanche=2">
                        <then>
                            <output expression="&quot;Escolha confirmada: Quarteir&#227;o.&quot;" newline="True"/>
                        </then>
                        <else>
                            <if expression="EscolhaLanche=3">
