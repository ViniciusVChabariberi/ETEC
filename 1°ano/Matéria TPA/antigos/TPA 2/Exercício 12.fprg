    <function name="Main" type="None" variable="">
        <parameters/>
        <body>
            <declare name="A, B, C" type="Integer" array="False" size=""/>
            <output expression="&quot;Programa para informar qual &#233; o tipo de tri&#226;ngulo&quot;" newline="True"/>
            <output expression="&quot;Informe o lado 1: &quot;" newline="True"/>
            <input variable="A"/>
            <output expression="&quot;Informe o lado 2: &quot;" newline="True"/>
            <input variable="B"/>
            <output expression="&quot;Informe o lado 3: &quot;" newline="True"/>
            <input variable="C"/>
