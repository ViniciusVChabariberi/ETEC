                <then>
                    <if expression="(A=B) &amp;&amp; (A=C) &amp;&amp; (B=C)">
                        <then>
                            <output expression="&quot;Tri&#226;ngulo equil&#225;tero&quot;" newline="True"/>
                        </then>
                        <else>
                            <if expression="(A&lt;&gt;B) &amp;&amp; (A&lt;&gt;C) &amp;&amp; (B&lt;&gt;C)">
                                <then>
                                    <output expression="&quot;Tri&#226;ngulo escaleno&quot;" newline="True"/>
                                </then>
                                <else>
                                    <output expression="&quot;Tri&#226;ngulo is&#243;sceles&quot;" newline="True"/>
                                </else>
                            </if>
                        </else>
                    </if>
                </then>
                <else>
                    <output expression="&quot;Tri&#226;ngulo falso&quot;" newline="True"/>
                </else>
            </if>
        </body>
